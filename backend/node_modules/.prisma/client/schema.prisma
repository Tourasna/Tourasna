generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id          String   @id
  email       String   @unique
  name        String?
  username    String?
  preferences Json?
  avatarUrl   String?
  createdAt   DateTime @default(now())
}
